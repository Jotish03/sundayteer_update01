     <%- include("../../partials/message") %>
        <main>
    
            <section class="py-5 text-center container">
                
               

                <div>
                    <form class="formstyle" action="/previousmorningteerresult/search" method="POST">
                       
                   
                            <label for="date" style="font-weight: bold;">Start Date</label>
                            <input  type="date" name="startdate" 
                                placeholder="Date">
                       
    
                       
                            <label class="endlablel" for="date" style="font-weight: bold;">End Date</label>
                            <input  type="date" name="enddate" \
                                placeholder="Date">
                        
                       
                        <button type="submit" class="button" >Search</button>
                    </form>
                </div>

                <%if(currentUser){%>
                    <%if(currentUser.role.includes('admin')){%>
                <a href="/previousmorningteerresult/addmorningteerresult" type="button" class="btn btn-sm mb-3  btn-dark">Add Morning Teer Result</a>
                <%}%>
                    <%}%>
                <div class="row no-gutters">
    
                    <table class="table table-bordered border border-dark">
    <h5 class="bg-dark text-white mb-0 p-3">Morning Result</h5>
                        <tbody>
                            <thead style="background-color: #99e4af;">
    
                                <th>City</th>
                                <th>Date & Time</th>
                                <th>First Round</th>
                                <th>Second Round</th>
                                <%if(currentUser){%>
                                    <%if(currentUser.role.includes('admin')){%>
                                <th>Action</th>
    <%}%>
        <%}%>
                            </thead>
                            <%result.morningteerresult.forEach((data)=>{%>
    
                                <tr>
    
                                    <td>
                                        <%=data.city%>
                                    </td>
                                    <td>
                                        <%=data.date%>
                                    </td>
                                    <td>
                                        <%=data.firstround%>
                                    </td>
                                    <td>
                                        <%=data.secondround%>
                                    </td>
                                    <%if(currentUser){%>
                                        <%if(currentUser.role.includes('admin')){%>
                                    <td>
                                        <form action="/previousmorningteerresult/<%=data._id %>/delete?_method=DELETE" method="POST">
                                            <button class="btn btn-sm  btn-dark text-center bg-white text-dark mb-3" class="">Delete</button>
                                        </form>
                                    </td>
                                <%}%>
                                    <%}%>
    
                                </tr>
                                <%})%>
                        </tbody>
                    </table>

                    <div class="pagination">
                    <%if(result.next &&result.next.page<=result.count){%>
                        <a  href="/previousmorningteerresult?page=<%=result.next.page%>&limit=30">
                            &raquo;
                        </a>
                        <%}%>
                            <%for(i=1; i<=result.count; i++){%>
                                <a  href="/previousmorningteerresult?page=<%=i%>&limit=30">
                                    <%=i%>
                                </a>
                                <%}%>
                
                                    <%if(result.previous){%>
                                        <a  href="/previousmorningteerresult?page=<%=result.previous.page%>&limit=30">
                                            
                                            &laquo;
                                        </a>
                                        <%}%>
                                    </div>

                </div>
